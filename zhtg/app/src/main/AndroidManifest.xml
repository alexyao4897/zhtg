<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.money.deep.tstock">


    <application
        android:name="com.money.deep.tstock.app.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme">


        <activity
            android:name="com.money.deep.tstock.activity.SplashActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.money.deep.tstock.activity.MainActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name="com.money.deep.tstock.activity.DetailActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.money.deep.tstock.activity.MinutesActivity"
            android:screenOrientation="portrait">
    </activity>
        <activity android:name="com.money.deep.tstock.activity.KLineActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.money.deep.tstock.activity.SearchActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.money.deep.tstock.activity.LoginActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.money.deep.tstock.activity.UserCenterActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.money.deep.tstock.activity.HelpActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.money.deep.tstock.activity.UserInfoActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.money.deep.tstock.activity.AgreeActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.money.deep.tstock.activity.KnowActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.money.deep.tstock.activity.AboutActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.money.deep.tstock.activity.GuideActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.money.deep.tstock.activity.AdviceActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.money.deep.tstock.activity.InAppWebViewActivity"
            android:screenOrientation="portrait">
        </activity>




        <!--alipay sdk-->
        <activity
            android:name="com.alipay.sdk.app.H5PayActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" >
        </activity>
        <activity
            android:name="com.alipay.sdk.auth.AuthActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" >
        </activity>
        <!--alipay end-->

    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

</manifest>
